  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^0.0.15",
    "@types/ini": "^1.3.29",
    "@types/mocha": "^2.2.41",
    "@types/yargs": "^8.0.1",
    "@types/lodash.chunk": "^4.2.2",
    "@types/node": "^8.0.14",
    "mocha": "^3.4.2",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "aws-sdk": "^2.88.0",
    "ini": "^1.3.4",
    "lodash.chunk": "^4.2.0",
    "yargs": "^8.0.2"
  }
