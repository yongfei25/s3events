{
  "name": "s3events",
  "version": "0.0.1",
  "description": "Command line tool to resend AWS S3 events to new or existing event handlers",
  "main": "index.js",
  "bin": {
    "s3events": "bin/s3events.js"
  },
  "scripts": {
    "build": "tsc -p .",
